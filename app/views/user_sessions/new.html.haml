%h1 UserSessions#new
%p Find me in app/views/user_sessions/new.html.haml
= form_for @user_session do |f|
  %p
    = f.error_messages
  %p
    = f.label :login
    = f.text_field :login
  %p
    = f.label :password
    = f.password_field :password
  %p
    = submit_tag :login_with_twitter, :name => :login_with_twitter
